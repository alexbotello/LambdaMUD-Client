{
    "env": {
        "commonjs": true,
        "es6": true
    },
    "plugins": ["prettier"],
    "extends": ["plugin:prettier/recommended"],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "semi": 0,
        "prettier/prettier": "error",
        "prettier/semi": "false"
    }
}